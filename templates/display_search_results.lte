{% autoescape false %}
<!--  module: jasm - internal frontend-template -->
{% if no_results == 1 %}
	<p>{{ no_results_msg }}</p>
{% else %}
	<p class="search_results_head">{{ search_results_head }}</p>
	
	{% for result in all_results %}
		<p>P: <a href='{{ result.link }}'>{{ result.page_title }}</a></p>
		{{ result.content }}
		<hr size=1 />
	{% endfor %}
	
{% endif %}
{% endautoescape %}